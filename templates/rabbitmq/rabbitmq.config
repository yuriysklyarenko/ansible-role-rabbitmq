%% {{ ansible_managed }}
[
  {kernel, [
    {inet_dist_use_interface,{127,0,0,1}}
   ]},
  {rabbit, [
   {tcp_listen_options, [binary, {packet,raw},
     {reuseaddr,true},
     {backlog,128},
     {nodelay,true},
     {exit_on_close,false},
     {keepalive,false}]},
     {hipe_compile, true},
     {default_user, <<"guest">>},
     {default_pass, <<"guest">>}
   ]},
   {rabbitmq_management_agent, [
     {force_fine_statistics, false}
   ]},
   {rabbitmq_management, [
     {listener, [{port, 15672}, {ip, "127.0.0.1"}]}
   ]}
].
